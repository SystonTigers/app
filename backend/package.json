{
  "name": "syston-backend",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "build": "esbuild src/index.ts --bundle --format=esm --outdir=dist --out-extension:.js=.mjs --define:APP_VERSION=\\\"${npm_package_version}\\\" && esbuild src/queue-consumer.ts --bundle --format=esm --outdir=dist --out-extension:.js=.mjs --define:APP_VERSION=\\\"${npm_package_version}\\\"",
    "dev": "wrangler dev --local",
    "deploy": "wrangler deploy",
    "d1:prep": "wrangler d1 execute DB --file ./src/schema/d1.sql",
    "test": "vitest --run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.4.0",
    "@cloudflare/workers-types": "^4.20241022.0",
    "@types/node": "^22.7.7",
    "esbuild": "^0.23.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.2",
    "vitest": "1.5.0",
    "wrangler": "^4.42.2"
  },
  "dependencies": {
    "@resvg/resvg-wasm": "^2.6.0",
    "isomorphic-dompurify": "^2.31.0",
    "itty-router": "^4.0.23",
    "jose": "^6.1.0",
    "luxon": "^3.5.0",
    "node-fetch": "^3.3.2",
    "uuid": "^9.0.1",
    "zod": "^3.25.76"
  }
}
